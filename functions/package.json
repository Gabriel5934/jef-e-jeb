{
  "name": "functions",
  "scripts": {
    "build": "sucrase ./src -d ./lib --transforms typescript,imports",
    "serve": "sucrase ./src -d ./lib --transforms typescript,imports && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "16.3.0"
  },
  "main": "lib/index.js",
  "dependencies": {
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "firebase-admin": "^9.8.0",
    "firebase-functions": "^3.14.1",
    "sucrase": "^3.18.2"
  },
  "devDependencies": {
    "firebase-functions-test": "^0.2.0",
    "typescript": "^3.8.0"
  },
  "private": true,
  "version": "1.0.0",
  "license": "MIT"
}
